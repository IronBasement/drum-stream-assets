    "@vitejs/plugin-react": "^4.3.1",
    "amqplib": "^0.10.5",
    "better-sqlite3": "^11.5.0",
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "discord.js": "^14.17.2",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "googleapis": "^144.0.0",
    "howler": "^2.2.4",
    "kysely": "^0.27.4",
    "kysely-d1": "^0.3.0",
    "midi": "^2.0.0",
    "node-id3tag": "^0.9.8",
    "open": "^10.1.0",
