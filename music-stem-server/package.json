    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/howler": "^2.2.11",
    "@types/midi": "^2.0.3",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/ws": "^8.5.12",
    "concurrently": "^9.1.2",
    "typescript": "^5.6.3"
